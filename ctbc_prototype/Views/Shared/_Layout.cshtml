<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta id="view-port" name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - ctbc_prototype</title>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/coreui.css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/css/coreui2.min.css" />
    <link rel="stylesheet" href="~/css/coreui3.min.css" />
    <link rel="stylesheet" href="~/css/datatables.min.css" />
    <link rel="stylesheet" href="~/css/select2.min.css" />
    @RenderSection("Styles", required: false)
    <style>
        .container-fluid {
            padding: 15px 0px 15px 15px !important;
        }

        .dashboard-Title {
            color: #fff;
            background-color: #e5755f;
        }

        .div-logo {
            margin-left: 0px;
            height: 100px;
            width: inherit;
            background-repeat: no-repeat;
            background-color: white;
            background-size: 300px 120px;
        }

        .div-Info {
            margin-bottom: -10px;
        }

        .nav-admin {
            background: #6d000e;
        }

        .sidebar .nav-dropdown.nav-admin.open {
            background: #6d000e;
        }

        .nav-admin-child {
            background: #b81a2d;
        }

        .nav-dropdown.open .nav-item.nav-admin-child {
            background: #b81a2d;
        }

        .sidebar .nav-dropdown.open .nav-item.nav-admin-child .nav-link.active {
            background: #ef8f9b;
        }
    </style>
</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show">
    <partial name="_NavbarPartial.cshtml" model="Model" />
    <partial name="_SidebarPartial.cshtml" model="Model" />
    <div class="app-body">
        <main role="main">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </main>
        @*<partial name="_AsideMenuPartial.cshtml" model="Model" />*@
    </div>
    <partial name="_FooterPartial.cshtml" model="Model" />
    <partial name="~/Views/Shared/_ModalDialogBox.cshtml" />
    <partial name="~/Views/Shared/_ModalAlertBox.cshtml" />
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/moment.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/modernizr.min.js"></script>
    <script src="~/js/select2.min.js"></script>
    <script src="~/js/coreuiDependency.min.js"></script>
    <script src="~/js/coreui.min.js"></script>
    <script src="~/js/datatables.min.js"></script>
    <script src="~/js/dialog.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
